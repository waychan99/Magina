# Uncomment the next line to define a global platform for your project
# platform :ios, '11.0'

target 'Magina' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Magina

  # 网络框架
  pod 'AFNetworking'
  # 模型转换
  pod 'MJExtension'
  # 键盘辅助工具
  pod 'IQKeyboardManager'
  # 导航栏
  pod 'WRNavigationBar'
  # 约束布局
  pod 'Masonry'
  # 加解密
  pod 'CocoaSecurity'
  # 加载蒙版
  pod 'SVProgressHUD'
  # 刷新控件
  pod 'MJRefresh'
  # 分栏控制器
  pod 'JXPagingView/Pager'
  pod 'JXCategoryView'
  # 空数据占位图
  pod 'LYEmptyView'
  # KVO block回调，自动销毁
  pod 'KVOController'
  # 异步加载图片
  pod 'SDWebImage'
  # 按钮图片文字位置灵活调整
  pod 'SPButton'
  # PanModal
  pod 'HWPanModal'


